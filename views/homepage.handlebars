<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
    <div class="carousel-inner">
        <div class="carousel-item active" data-interval="2000">
            <img class="d-block w-100" src="./frame17.png" alt="First slide">
        </div>
        <div class="carousel-item" data-interval="2000">
            <img class="d-block w-100" src="./frame16.png" alt="Second slide">
        </div>
        <div class="carousel-item" data-interval="2000">
            <img class="d-block w-100" src="./frame15.png" alt="Third slide">
        </div>
    </div>
</div>

<!-- Market Stats -->
<div class="uk-width-2-3@m">
    <div class="uk-card uk-card-default uk-card-body">
        <h2 class="uk-text-center uk-text-large"><i class="fas fa-chart-area"></i>Folio Stock Picks of the Week</h2>
        <table class="uk-table uk-table-divider table table-dark">
            <thead>
                <tr>
                    <th>Stock</th>
                    <th>Price</th>
                    <th>Rating</th>


                </tr>
            </thead>
            <tbody>
                {{#each stocks as |stock|}}
                <tr>
                    <td>{{stock.symbol}}</td>
                    <td>{{stock.price}}</td>
                    <td>{{stock.rating}}</td>

                </tr>
                {{/each}}
            </tbody>
        </table>

    </div>
</div>
</div>

<script src="https://cdnjs.cloudflare.com/ajax/libs/moment.js/2.20.1/moment.min.js"></script>
